  "description": "Cloud Data",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "start:win32": "electron-forge start",
    "package-cli": "pkg ./src/cli/index.js --targets node14-linux-x64,node14-mac-x64,node14-win-x64 --output ./dist-cli/cloud-data-cli",
    "package": "electron-forge package",
    "package:linux": "electron-forge package --platform=linux",
    "package:macos": "electron-forge package --platform=darwin",
    "package:win32": "electron-forge package --arch=ia32 --platform=win32",
    "package:win64": "electron-forge package --arch=x64 --platform=win32",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
