/*
===============================================================================
  Commons
===============================================================================
*/
data "aws_caller_identity" "current" {}
data "aws_region" "current" {}

/*
===============================================================================
  AWS VPC
===============================================================================
*/
data "aws_vpc" "this" {
  id = var.vpc_id
}

data "aws_subnets" "this" {
  filter {
    name   = "subnet-id"
    values = var.subnet_ids
  }
}
